{
  "name": "@brightcodeui/beta-ui",
  "version": "0.0.18",
  "description": "A TypeScript UI Component library for developers, Supports React JS, Next JS, Angular JS etc.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "prepare": "husky",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "release": "standard-version",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brightphi210/brightui.git"
  },
  "author": "Bright Philip",
  "license": "MIT",
  "homepage": "https://github.com/brightphi210/brightui.git#readme",
